# Copy this file to .env and fill in your values

# Environment Mode
# Valid values: local, staging, production
ENV_MODE=local

# DAILY CREDITS CONFIGURATION
# IMPORTANT: FREE users get NO daily credits, PAID users get non-cumulative daily credits

# Amount of daily credits for PAID users (default: 200 = $2 worth)
# These credits DO NOT accumulate - new credits replace old ones
DAILY_CREDITS_AMOUNT=200

# Initial one-time credits for FREE users (default: 500)
# After these are used, FREE users get NO more credits
FREE_TRIAL_CREDITS=500

# Daily credits are ONLY for paid users (MUST be true in production)
DAILY_CREDITS_PAID_ONLY=true

# Block agent execution when user has no credits (recommended: true)
BLOCK_WHEN_NO_CREDITS=true

